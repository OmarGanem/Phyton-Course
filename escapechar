splitString = "this string has been\nsplit over\nseveral\nlines"
print(splitString)

tabbedString = "1t\t2\t3\t4\t5"
print(tabbedString)
print('The pet shop owner said "no, no \'e\'s uh,...he\'s resting".')
#or
print("the pet shop owner said \"no, no 'e's uh,... he's resting\".")

print("""the pet shop owner said"no, no, \
    'e's uh...he's resting""")

